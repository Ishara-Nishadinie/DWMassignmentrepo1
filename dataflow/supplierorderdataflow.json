{
	"name": "supplierorderdataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSqlSinkTableSuliper",
						"type": "DatasetReference"
					},
					"name": "supplier"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTableOrderFact",
						"type": "DatasetReference"
					},
					"name": "orderdata"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Supplier_SK as integer,",
				"          Supplier_Code as string,",
				"          Name as string,",
				"          Region as string,",
				"          Country as string,",
				"          Type as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 20,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> supplier",
				"source(output(",
				"          Order_Id as string,",
				"          Product_SK as string,",
				"          Customer_SK as string,",
				"          Order_Date_SK as string,",
				"          Order_Suppliers_SK as string,",
				"          Status_SK as string,",
				"          Quantity as integer,",
				"          Unit_Cost as decimal(10,2)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     limit: 20,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> orderdata"
			]
		}
	}
}